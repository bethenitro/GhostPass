# GhostPass Stress Test Environment Configuration
# Copy this file to .env.test and fill in your values

# API Configuration
API_BASE_URL=https://your-ghostpass-api.vercel.app

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_KEY=your-service-role-key-here

# Test Venue Configuration
TEST_VENUE_ID=test-venue-001
TEST_GATEWAY_1_ID=gateway-door-1
TEST_GATEWAY_2_ID=gateway-door-2

# Stripe Configuration (use test keys)
STRIPE_TEST_KEY=sk_test_...
STRIPE_WEBHOOK_SECRET=whsec_test_...

# Test Configuration
TEST_DURATION_MINUTES=25
TEST_WALLET_COUNT=5000
TEST_CONCURRENT_USERS=2000

# Monitoring Configuration
ENABLE_DETAILED_LOGGING=true
LOG_LEVEL=info

# Optional: Grafana Cloud for k6 results
K6_CLOUD_TOKEN=your-k6-cloud-token
